{
  "name": "FormPress",
  "version": "0.1.0",
  "description": "OpenSource Data collection solution",
  "main": "app.js",
  "scripts": {
    "test": " mocha ./test/**/*.spec.js",
    "migrate": "node script/migrate.js",
    "start-dev": "yarn migrate && nodemon app.js --ignore script/transformed",
    "start": "yarn migrate && node app.js",
    "rebuild-backend": "docker-compose stop backend && docker-compose rm -f backend && docker-compose build backend && docker-compose create backend && docker-compose start backend",
    "restart-backend": "docker-compose stop backend && docker-compose start backend"
  },
  "author": "kemal@formpress.org",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-transform-modules-commonjs": "7.7.5",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-react": "7.7.4",
    "@babel/register": "7.7.7",
    "@sendgrid/mail": "6.4.0",
    "ejs": "3.0.1",
    "express": "4.17.1",
    "express-basic-auth": "^1.2.0",
    "ignore-styles": "5.0.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "2.0.2",
    "promise-mysql": "4.1.1",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "mocha": "7.0.0",
    "sinon": "8.1.0",
    "supertest": "4.0.2"
  }
}
